# Aggressive network retry settings for Netlify
fetch-timeout=600000
fetch-retries=15
fetch-retry-factor=1.5
fetch-retry-mintimeout=60000
fetch-retry-maxtimeout=300000

# Cache settings
cache=.npm-cache
prefer-offline=true

# Registry settings
registry=https://registry.npmjs.org/
strict-ssl=false

# Disable unnecessary features
audit=false
fund=false
optional=false
